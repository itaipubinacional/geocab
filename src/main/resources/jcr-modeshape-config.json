{
    "name" : "geocab-repository",
    "workspaces" : {
        "default" : "default",
        "allowCreation" : true
    },
    "security" : {
        "anonymous" : {
            "roles" : ["readonly","readwrite","admin"],
            "useOnFailedLogin" : false
        }
    },
    "storage" : {
        "binaryStorage" : {
            "type" : "file",
            "directory": "${files.repository.path}",
            "minimumBinarySizeInBytes" : 40
        }
    }
}